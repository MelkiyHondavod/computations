def dividors_tail(Numb, div):
  if (Numb%div == 0):
    print (div)
    return dividors_tail (Numb//div, div)
  
  elif(Numb==1):
    return

  else:
    return dividors_tail (Numb, div+1)


def dividors_tail_while(Numb, div):
  while(Numb != 1):
    if (Numb%div == 0):
      print (div)
      Numb = Numb//div

    else:
      div = div+1
  
